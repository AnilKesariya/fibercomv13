<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="partner_analytic_view" model="ir.ui.view">
			<field name="name">partner.analytic.view</field>
			<field name="model">res.partner</field>
			<field name="inherit_id" ref="base.view_partner_form"/>
			<field name="arch" type="xml">
				<xpath expr="//form//sheet//group[1]//group[2]//field[@name='category_id']" position="after">
					<field name="analytic_id"/>
				</xpath>
			</field>
		</record>
	</data>
</odoo>